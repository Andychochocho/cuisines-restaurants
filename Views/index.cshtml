<h1>Best Restaurants in Luxenburg!</h1>

<h1>Check the options below for cuisine.</h1>

@if (@Model.Count == 0)
{
<h2>You have failed to add any cusisines!</h2>
}
<ul>
  @foreach (var cuisinename in Model)
  {
    <li><a href="/cuisines/@cuisinename.GetId()">@cuisinename.GetName()</a></li>
  }
</ul>
<a href="/cuisine/new">Add a new cuisine</a>
